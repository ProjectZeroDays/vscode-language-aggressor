{
    "name": "aggressorscript",
    "displayName": "AggressorScript",
    "description": "Cobalt Strike Aggressor Script Language Extension for Visual Studio Code.",
    "homepage": "https://github.com/darkoperator/vscode-language-aggressor",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "cna",
            "aliases": ["AggressorScript", "cna"],
            "extensions": ["cna"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cna",
            "scopeName": "source.cna",
            "path": "./syntaxes/cna.tmLanguage.json"
        }],
        "snippets": [{
            "language": "cna",
            "path": "./snippets/cna.json"
        }]
    },
    "author": {
        "name": "Carlos Perez",
        "url": "https://www.darkoperator.com/"
    },
    "contributors": [{
        "name": "Paul Laîné",
        "url": "https://twitter.com/am0nsec"
    }]
}